  ],
  "author": "info@orbisops.com",
  "main": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "license": "MIT",
  "files": [
    "dist/*"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/orbisoperations/lambda-layer-typeorm-data-api"
  },
  "engines": {
